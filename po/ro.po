msgid "Radio device"
msgstr "Dispozitiv radio"

#: ../data/gnomeradio.schemas.in.h:13
msgid "Specify the device driver to use (any, v4l1 or v4l2)"
msgstr "Specifică driverul dispozitivului folosit („any”, „v4l1” sau „v4l2”)"

#: ../data/gnomeradio.schemas.in.h:14 ../src/prefs.c:626
msgid "Specify the radio-device (in most cases /dev/radio)"
msgstr "Dispozitivul radio folosit (de obicei /dev/radio)"

