#: gnomeradio.schemas.in.h:1
msgid "Audio device"
msgstr "Soinu-gailua"

#: gnomeradio.schemas.in.h:2 src/record.c:353
msgid "Choose the bitrate in which the mp3 will be encoded"
msgstr "Hautatu Mp3a kodetzeko erabiliko den bit-tasa"

#: gnomeradio.schemas.in.h:3 src/prefs.c:699
msgid ""
"Choose the mixer source (line, line1, etc.) that is able to control the "
"volume of your radio"
msgstr ""
"Hautatu zure irratiaren bolumena kontrolatzen duen nahasgailuaren iturria "
"(line, line1, e.a.)"

#: gnomeradio.schemas.in.h:4 src/record.c:352
msgid "Choose the mp3 encoder that should be used"
msgstr "Hautatu erabiliko den mp3 kodetzailea"

#: gnomeradio.schemas.in.h:5 src/prefs.c:700
msgid "If unchecked, gnomeradio won't mute after exiting"
msgstr "Aukeratu ezean, gnomeradio ez da isilduko irten eta gero"

#: gnomeradio.schemas.in.h:6
msgid "Last frequency"
msgstr "Azken frekuentzia"

#: gnomeradio.schemas.in.h:7
msgid "Last presets"
msgstr "Azken ezarpenak"

#: gnomeradio.schemas.in.h:8
msgid "MP3 bitrate"
msgstr "MP3 bit-tasa"

#: gnomeradio.schemas.in.h:9
msgid "MP3 encoder"
msgstr "MP3 kodetzailea"

#: gnomeradio.schemas.in.h:10
msgid "Mixer Device"
msgstr "Nahasgailua"

#: gnomeradio.schemas.in.h:11
msgid "Mixer source"
msgstr "Nahasgailuaren iturria"

#: gnomeradio.schemas.in.h:12
msgid "Mute on exit"
msgstr "Isildu irtetean"

#: gnomeradio.schemas.in.h:13
msgid "Number of presets"
msgstr "Ezarpen-kopurua"

#: gnomeradio.schemas.in.h:14
msgid "Radio device"
msgstr "Irrati-gailua"

#: gnomeradio.schemas.in.h:15
msgid "Record as mp3- or wave-file"
msgstr "Grabatu Mp3 edo wave-fitxategi gisa"

#: gnomeradio.schemas.in.h:16
msgid "Record in stereo or mono"
msgstr "Grabatu Mono edo estereo moduan"

#: gnomeradio.schemas.in.h:17
msgid "Sample format"
msgstr "Lagin-formatua"

#: gnomeradio.schemas.in.h:18
msgid "Sample rate"
msgstr "Lagin-tasa"

#: gnomeradio.schemas.in.h:19 src/prefs.c:697
msgid "Specify the radio-device (in most cases /dev/radio)"
msgstr "Zehaztu irrati-gailua (normalean /dev/radio)"

#: gnomeradio.schemas.in.h:20 src/record.c:354
msgid "The audio device to use (usually /dev/audio)"
msgstr "Erabiliko den soinu-gailua (normalean /dev/audio)"

#: gnomeradio.schemas.in.h:21 src/record.c:350
msgid "The audio sample-rate of the wave-file"
msgstr "Wave fitxategiko soinuaren lagin-tasa"

#: gnomeradio.schemas.in.h:22 src/record.c:351
msgid "The format (8 or 16 bit) of the wave-file"
msgstr "Wave fitxategiaren formatua (8 edo 16 bitekoa)"

#: gnomeradio.schemas.in.h:23
msgid "The last frequency (in MHz) in use"
msgstr "Erabilitako azken frekuentzia (Mhz-tan)"

#: gnomeradio.schemas.in.h:24
msgid ""
"The mixer device that gnomeradio should use. Only changeable via this key "
"(no GUI option)"
msgstr ""
"Gnomeradiok erabiliko duen nahasgailua. Bakarrik gako honen bitartez alda "
"daiteke (ez dago aukera grafikorik)"

#: gnomeradio.schemas.in.h:25
msgid "The number of presets"
msgstr "Ezarpenen kopurua"

#: gnomeradio.schemas.in.h:26
msgid ""
"The preset that was enabled when gnomeradio was terminated. Range is 0 ... "
"(# presets - 1). None is -1"
msgstr ""
"Gnomeradio irtetean gaitutako ezarpena. Bitartea 0 da ... (# ezarpenak - 1). "
"Bat ere ez -1 da"

#: gnomeradio.schemas.in.h:27
msgid "The stored value of the volume (0-100)"
msgstr "Gordetako bolumenaren balioa (0-100)"

#: gnomeradio.schemas.in.h:28
msgid "Volume"
msgstr "Bolumena"

#: gnomeradio.schemas.in.h:29 src/record.c:355 src/record.c:356
msgid "Whether the output should be a mp3- or a wave-file"
msgstr "Irteera-formatua mp3- edo wave-fitxategia izango den adierazten du"

#: gnomeradio.schemas.in.h:30 src/record.c:357 src/record.c:358
msgid "Whether the output should be in stereo or mono"
msgstr "Irteera-formatua estereo edo mono izango den adierazten du"

